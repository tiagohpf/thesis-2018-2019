{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "schoolFrontEnd-v2": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "targets": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/schoolFrontEnd-v2",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/assets/botschool/botschool.css",
              "src/assets/botschool/fuxicons.css",
              "src/assets/webct/libs/leaflet/leaflet.css",
              "src/assets/webct/libs/primeng/primeng.min.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/@ng-select/ng-select/themes/default.theme.css",
              "src/assets/webct/libs/font-awesome/css/font-awesome.min.css",
              "src/assets/webct/css/datepicker.css",
              "src/assets/webct/css/toaster.css",
              "src/assets/webct/css/webct-icons.css",
              "src/assets/webct/css/wct-app.css",
              "src/assets/ablepro_theme/css/main.css",
              "src/assets/css/theme.scss",
              "src/assets/css/subscriptions.scss",
              "src/assets/css/entities/dialogDesigner/main.scss",
              "src/assets/css/botschool.scss"
            ],
            "scripts": [
              "src/assets/webct/libs/jquery/jquery.min.js",
              "src/assets/libs/basepack/web/modernizr/2.6/modernizr.js",
              "src/assets/libs/fuxi/lib/uilayout/jquery.layout-latest.js",
              "src/assets/libs/fuxi/common/exec/fuxi.min.js",
              "src/assets/webct/libs/bootstrap/js/bootstrap.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "schoolFrontEnd-v2:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "schoolFrontEnd-v2:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "schoolFrontEnd-v2:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "scripts": [
              "src/assets/webct/libs/jquery/jquery.min.js",
              "src/assets/libs/basepack/web/modernizr/2.6/modernizr.js",
              "src/assets/libs/fuxi/lib/uilayout/jquery.layout-latest.js",
              "src/assets/libs/fuxi/common/exec/fuxi.min.js",
              "src/assets/webct/libs/bootstrap/js/bootstrap.min.js"
            ],
            "styles": [
              "src/styles.css",
              "src/assets/webct/libs/leaflet/leaflet.css",
              "src/assets/webct/libs/primeng/primeng.min.css",
              "src/assets/webct/libs/font-awesome/css/font-awesome.min.css",
              "src/assets/webct/css/datepicker.css",
              "src/assets/webct/css/toaster.css",
              "src/assets/webct/css/webct-icons.css",
              "src/assets/webct/css/wct-app.css"
            ],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "schoolFrontEnd-v2-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "targets": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "schoolFrontEnd-v2:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "schoolFrontEnd-v2:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "schoolFrontEnd-v2",
  "schematics": {
    "@schematics/angular:component": {
      "styleext": "scss"
    }
  }
}