{
  "type": "page",
  "id": "pageEntityDetails",
  "urlResource": [
    {
      "key": "path",
      "value": "{{SchoolBOT}}/students/[[id]]"
    },
    {
      "key": "pathToMainData",
      "value": [
        ""
      ]
    },
    {
      "key": "reload",
      "value": false
    },
    {
      "key": "headers",
      "value": {
        "Authorization": "Bearer {{token}}",
        "IXS": "{{IXS_id}}"
      }
    }
  ],
  "header": {
    "id": "pageHeader",
    "details": [
      {
        "type": "label",
        "text": null,
        "class": "col-md-12 back",
        "value": "<i class='icomoon icon-go' ></i> <span>{{@fv()@i18n('generic___operations___back')}}</span>",
        "navigateTo": "{{wct-previous-route}}"
      },
      {
        "type": "context",
        "id": "studentID",
        "pathToValue": [
          "_id",
          "$oid"
        ]
      },
      {
        "type": "context",
        "id": "studentCoursesList",
        "pathToValue": [
          "@getParameterByIDFilter($this['courses'])"
        ]
      },
      {
        "type": "context",
        "id": "studentLanguage",
        "text": "application___language",
        "pathToValue": [
          "language"
        ]
      },
      {
        "type": "context",
        "id": "studentGender",
        "text": "student___gender",
        "pathToValue": [
          "gender"
        ]
      },
      {
        "type": "context",
        "id": "studentCourse",
        "text": "studentCourse",
        "pathToValue": [
          "course"
        ]
      },
      {
        "type": "context",
        "id": "studentDescription",
        "text": "student___description",
        "class": "col-md-12",
        "pathToValue": [
          "description"
        ]
      },
      {
        "type": "wrapper",
        "id": "imageLabel",
        "pathToValue": "",
        "details": [
          {
            "id": "entityInfoHeader",
            "type": "uploadImages",
            "class": "col-md-12",
            "pathToValue": [
              "@createStudentObj(@replaceTagVars('studentID'),@replaceTagVars('studentName'),@replaceTagVars('studentStatus'),@replaceTagVars('studentGender'),@replaceTagVars('studentLanguage'),@replaceTagVars('studentCourse'),'{{ImageService}}/students/{{studentID}}')"
            ],
            "readonly": false
          },
          {
            "type": "label",
            "id": "studentStatus_aux",
            "pathToValue": "status",
            "hidden": true
          },
          {
            "type": "label",
            "text": null,
            "dynamicProps": {
              "class": "'col-md-12 entityStatus {{studentStatus_aux}}'"
            },
            "pathToValue": [
              "imageLabel",
              "status"
            ],
            "html": {
              "pre-live": "{{@fv()@i18n('student___status___gettingReady')}}",
              "live": " {{@fv()@i18n('student___status___live')}}",
              "blocked": "{{@fv()@i18n('student___status___blocked')}}",
              "suspended": "{{@fv()@i18n('student___status___suspended')}}"
            }
          }
        ]
      },
      {
        "type": "label",
        "text": null,
        "class": "col-md-12 entityName",
        "pathToValue": [
          "name"
        ]
      },
      {
        "type": "label",
        "text": null,
        "class": "col-sm-12 entityCourse",
        "urlResource": [
          {
            "key": "path",
            "value": "{{SchoolBOT}}/courses"
          },
          {
            "key": "pathToMainData",
            "value": [
              "_embedded"
            ]
          },
          {
            "key": "queryString",
            "value": {
              "filter": "{ \"_id\":{\"$in\":{{studentCoursesList}} }}"
            }
          },
          {
            "key": "reload",
            "value": false
          },
          {
            "key": "headers",
            "value": {
              "Authorization": "Bearer {{token}}",
              "IXS": "{{IXS_id}}"
            }
          }
        ],
        "pathToValue": [
          "name"
        ]
      },
      {
        "type": "context",
        "text": null,
        "id": "courseID",
        "class": "col-md-12",
        "pathToValue": [
          "$this['courses'][0]"
        ]
      },
      {
        "type": "context",
        "global": true,
        "id": "studentData",
        "pathToValue": [
          "new Object({\"studentId\":$this['_id']['$oid'], \"studentName\": $this['name']})"
        ]
      },
      {
        "type": "operations",
        "id": "courseButton",
        "class": "col-md-12 courseButton",
        "text": null,
        "operations": [
          {
            "size": 10,
            "parameters": [
