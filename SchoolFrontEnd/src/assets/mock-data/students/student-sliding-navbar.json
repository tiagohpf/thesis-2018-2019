{
  "type": "initParameters",
  "id": "pageEntityDetails_navBar",
  "urlResource": [
    {
      "key": "path",
      "value": "{{SchoolBOT}}/students/[[id]]"
    },
    {
      "key": "pathToMainData",
      "value": [
        ""
      ]
    },
    {
      "key": "reload",
      "value": false
    },
    {
      "key": "headers",
      "value": {
        "Authorization": "Bearer {{token}}",
                        "IXS": "{{IXS_id}}"
      }
    }
  ],
  "details": [
    {
      "type": "wrapper",
      "id": "pageEntityDetails_wrapper",
      "class": "col-sm-12",
      "pathToValue": "",
      "details": [
        {
          "type": "label",
          "text": null,
          "class": "col-sm-2 back",
          "value": "<i class='icomoon icon-go' ></i> <span>{{@fv()@i18n('generic___operations___back')}}</span>",
          "navigateTo": "{{wct-previous-route}}"
        },
        {
          "type": "wrapper",
          "id": "imageLabel_navBar",
          "class": "col-sm-2",
          "pathToValue": [
            "pageEntityDetails_wrapper"
          ],
          "details": [
            {
              "id": "entityInfoHeader",
              "type": "uploadImages",
              "class": "col-sm-8",
              "pathToValue": [
                "@createStudentObj(@replaceTagVars('studentID'),@replaceTagVars('studentName'),@replaceTagVars('studentStatus'),@replaceTagVars('studentGender'),@replaceTagVars('studentLanguage'),@replaceTagVars('studentCourse'),'{{ImageService}}/students/{{studentID}}')"
              ],
              "readonly": false
            },
            {
              "type": "label",
              "id": "studentStatus_aux",
              "pathToValue": "status",
              "hidden": true
            },
            {
              "type": "label",
              "text": null,
              "dynamicProps": {
                "class": "'col-md-4 entityStatus {{studentStatus_aux}}'"
              },
              "pathToValue": [
                "imageLabel",
                "status"
              ],
              "html": {
                "pre-live": "{{@fv()@i18n('student___status___gettingReady')}}",
                "live": " {{@fv()@i18n('student___status___live')}}",
                "blocked": "{{@fv()@i18n('student___status___blocked')}}",
                "suspended": "{{@fv()@i18n('student___status___suspended')}}"
            }
            }
          ]
        },
        {
          "type": "wrapper",
          "id": "nameLabel_navBar",
          "class": "col-sm-6",
          "pathToValue": [
            "pageEntityDetails_wrapper"
          ],
          "details": [
            {
              "type": "label",
              "text": null,
              "class": "col-md-12 entityName",
              "pathToValue": [
                "name"
              ]
            },
            {
              "type": "context",
              "id": "studentCourse",
              "text": "studentCourse",
              "pathToValue": [
                "course"
              ]
            },
            {
              "type": "context",
              "id": "studentCoursesList",
              "pathToValue": [
                "@getParameterByIDFilter($this['courses'])"
              ]
            },
            {
              "type": "context",
              "id": "studentCoursesID",
              "pathToValue": [
                "$this['courses'][0]"
              ]
            },
            {
              "type": "label",
              "id": "courseStudents",
              "text": null,
              "class": "col-sm-6 entityCourse",
              "urlResource": [
                {
                  "key": "path",
                  "value": "{{SchoolBOT}}/courses"
                },
                {
                  "key": "pathToMainData",
                  "value": [
                    "_embedded"
                  ]
                },
                {
                  "key": "queryString",
                  "value": {
                    "filter": "{ \"_id\":{\"$in\":{{studentCoursesList}} }}"
                  }
                },
                {
                  "key": "reload",
                  "value": false
                },
                {
                  "key": "headers",
                  "value": {
                    "Authorization": "Bearer {{token}}",
                        "IXS": "{{IXS_id}}"
                  }
                }
              ],
              "pathToValue": [
                "name"
              ]
            }
          ]
        },
        {
          "type": "operations",
          "id": "courseButton_navBar",
          "class": "col-md-2 courseButton",
          "operations": [
            {
              "size": 10,
              "parameters": [
                {
                  "type": "navigate",
                  "id": "studentCourses",
                  "text": "application___course",
                  "navigateTo": "knowledgecenter/courses/{{studentCoursesID}}"
                }
              ]
            }
          ]
        }
      ]
