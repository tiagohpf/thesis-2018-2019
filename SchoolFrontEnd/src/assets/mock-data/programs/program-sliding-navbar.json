              "id": "entityInfoHeader",
              "type": "uploadImages",
              "class": "col-sm-8",
              "pathToValue": [
                "@createProgramObj(@replaceTagVars('programID'),@replaceTagVars('programName'),@replaceTagVars('programStatus'),@replaceTagVars('programLanguage'),@replaceTagVars('programCategories'),'{{ImageService}}/programs/{{programID}}')"
              ],
              "readonly": false
            },
            {
              "type": "label",
              "id": "programStatus_aux",
              "pathToValue": "status",
              "hidden": true
          },
          {
              "type": "label",
              "id": "programStatus",
              "text": null,
              "dynamicProps": {
                  "class": "'col-md-4 entityStatus status_{{programStatus_aux}}'"
              },
              "pathToValue": [
                "status"
              ],
              "html": {
                "1": "<span class='statusText'>{{@fv()@i18n('program___status_types___new')}}</span>",
                "3": "<span class='statusText'>{{@fv()@i18n('program___status_types___published')}}</span>",
                "4": "<span class='statusText'>{{@fv()@i18n('program___status_types___editing')}}</span>"
              }
