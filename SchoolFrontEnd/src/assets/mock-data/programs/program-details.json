    "operations": [
      {
        "id": "programOperations",
        "size": 2,
        "role": "@fv()window.location.href.includes('programs')",
        "parameters": [
          {
            "id": "subscriptionPublishProgram",
            "type": "openModal",
            "text": "Publish",
            "class": "publish-button",
            "size": "lg",
            "mockToLoad": "programs/popUp-confirm-publish"
          },
          {
            "id": "removeProgram",
            "type": "openModal",
            "icon": "fa fa-trash-o",
            "mockToLoad": "programs/program-remove",
            "_role": "Programs.Operations.Remove",
            "parameters": [
              {
                "key": "context",
                "value": [
                  "programName"
                ]
              }
            ],
            "dynamicProps": {
              "disabled": "'{{programStatus_aux}}' != '1' && '{{programStatus_aux}}' != '2'"
            }
          }
        ]
      }
