{
  "type": "page",
  "id": "pageEntityDetails",
  "header": {
    "details": [
      {
        "type": "label",
        "text": null,
        "class": "col-md-12 back",
        "value": "<i class='icomoon icon-go' ></i> <span>{{@fv()@i18n('generic___operations___back')}}</span>",
        "navigateTo": "knowledgecenter"
      },
      {
        "type": "context",
        "role": "@fv()window.location.href.includes('students')",
        "id": "programStatus",
        "hidden": true,
        "pathToValue": [
          "status"
        ],
        "urlResource": [
          {
            "key": "path",
            "value": "{{SchoolBE}}/programs/[[programID]]"
          },
          {
            "key": "queryString",
            "value": {
              "keys": "{\"_id\":1,\"name\":1,\"status\":1}"
            }
          },
          {
            "key": "headers",
            "value": {
              "Authorization": "Bearer {{token}}",
              "IXS": "{{IXS_id}}"
            }
          }
        ]
      },
      {
        "type": "context",
        "id": "programID",
        "pathToValue": [
          "_id",
          "$oid"
        ]
      },
      {
        "comment": "Aplicavel na consulta dum program de um student",
        "type": "context",
        "role": "@fv()window.location.href.includes('students')",
        "id": "programID",
        "hidden": true,
        "pathToValue": [
          "programId"
        ]
      },
      {
        "type": "context",
        "id": "programName",
        "class": "col-sm-12 hide-label",
        "text": "program___name",
        "pathToValue": [
          "name"
        ]
      },
      {
        "type": "context",
        "id": "programStatus",
        "text": "program___name",
        "pathToValue": [
          "status"
        ]
      },
      {
        "type": "context",
        "id": "programCategories",
        "text": "programCategories",
        "pathToValue": [
          "categories"
        ]
      },
      {
        "type": "context",
        "id": "programLanguage",
        "text": "application___language",
        "pathToValue": [
          "language"
        ]
      },
      {
        "type": "context",
        "id": "programDescription",
        "text": "program___description",
        "class": "col-md-12",
        "pathToValue": [
          "description"
        ]
      },
      {
        "type": "wrapper",
        "id": "imageLabel",
        "pathToValue": "",
        "details": [
          {
            "id": "entityInfoHeader",
            "type": "uploadImages",
            "class": "col-md-12",
            "pathToValue": [
              "@createProgramObj(@replaceTagVars('programID'),@replaceTagVars('programName'),@replaceTagVars('programStatus'),@replaceTagVars('programLanguage'),@replaceTagVars('programCategories'),'{{ImageService}}/programs/{{programID}}')"
            ],
            "readonly": false
          },
          {
            "type": "label",
            "id": "programStatus_aux",
            "pathToValue": "status",
            "hidden": true
          },
          {
            "type": "label",
            "id": "programStatus",
            "text": null,
            "dynamicProps": {
              "class": "'col-md-12 entityStatus status_{{programStatus_aux}}'"
            },
            "pathToValue": [
              "status"
            ],
            "html": {
              "1": "<span class='statusText'>{{@fv()@i18n('program___status_types___new')}}</span>",
              "2": "<span class='statusText'>{{@fv()@i18n('program___status_types___new')}}</span>",
              "3": "<span class='statusText'>{{@fv()@i18n('program___status_types___published')}}</span>",
              "4": "<span class='statusText'>{{@fv()@i18n('program___status_types___editing')}}</span>"
            }
          }
        ]
      },
      {
        "type": "label",
        "text": null,
        "class": "col-md-12 entityName",
        "pathToValue": [
          "name"
        ]
      },
      {
        "type": "operations",
        "class": "col-md-12 courseButton",
        "text": null,
        "operations": [
          {
            "parameters": [
