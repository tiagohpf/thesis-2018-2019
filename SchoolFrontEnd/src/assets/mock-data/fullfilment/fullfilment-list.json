                                        {
                                            "id": "removeService",
                                            "type": "openModal",
                                            "text": "generic___operations___remove",
                                            "icon": "fa fa-trash-o",
                                            "mockToLoad": "fullfilment/fullfilment-remove",
                                            "parameters": [
                                                {
                                                    "key": "context",
                                                    "value": [
                                                        "id",
                                                        "name"
                                                    ]
